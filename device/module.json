{
  "type": "device",
  "build": [
    {
      "relativeDir": ".",
      "cmd": "./aos-init.sh"
    },
    {
      "relativeDir": "AliOS-Things",
      "cmd": "aos make helloworld@esp32devkitc"
    }
  ],

  "downloadConfigs": {
    "boardType": "esp32",
    "imageFiles": [
      "device/AliOS-Things/out/helloworld@esp32devkitc/binary/helloworld@esp32devkitc.bin"
    ]
  }
}
